CREATE TABLE NMT (
    ID INTEGER NOT NULL,
    H SMALLINT);
	

SET TERM ^ ;
CREATE PROCEDURE wstawdane (ile INTEGER) AS
DECLARE VARIABLE i INTEGER;
BEGIN
i=0;
while(i<ile) do
begin
INSERT INTO nmt (h) VALUES (FLOOR(RAND()*1400.0)+100);
i=i+1;
end
END
^
SET TERM ; ^
	
	
	
	
CREATE SEQUENCE GEN_NMT_ID;

create trigger nmt_bi for nmt
active before insert position 0
as
begin
  if (new.id is null) then
    new.id = gen_id(gen_nmt_id,1);
end



alter table NMT
add constraint PK_NMT
primary key (ID);